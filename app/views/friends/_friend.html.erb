<div id="<%= dom_id friend %>">
  <p>
    <strong>Firstname:</strong>
    <%= friend.first_name %>
  </p>

  <p>
    <strong>Lastname:</strong>
    <%= friend.last_name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= friend.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= friend.phone %>
  </p>

  <p>
    <strong>Twitter:</strong>
    <%= friend.twitter %>
  </p>
  <p>
    <strong>User Id:</strong>
      <%= friend.user_id %>
  </p>

</div>
